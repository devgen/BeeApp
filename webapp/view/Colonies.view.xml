<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:html="http://www.w3.org/1999/xhtml" controllerName="BeeApp.controller.Colonies">
	<App>
		<pages>
			
			<Page title="Title" showNavButton="true" navButtonPress="onNavBack">
 				<content>
						<IconTabBar id="centralTabBar" expanded="{device>isNoPhone}" class="sapUiResponsiveContentPadding">
						<items>
							<IconTabFilter text="Colonien">
								<Table id="arrivingFlights" items="{/Colony}" growing="true" growingScrollToLoad="true"> 
		
		<headerToolbar>
			<Toolbar>
				<Button icon="sap-icon://write-new-document" press="onOpenDialog"/>
				<Title text="Title"/>
				<ToolbarSpacer/>
				<SearchField width="50%" search="onFilterInvoices" selectOnFocus="false"/>
			</Toolbar>
		</headerToolbar>
									<columns>
										<Column>
											<Text text="Text"/>
										</Column>
										<Column>
											<Text text="Text"/>
										</Column>
										<Column>
											<Text text="Text"/>
										</Column>
										<Column>
											<Text text="Text"/>
										</Column>
										<Column>
											<Text text="Text"/>
										</Column>
										<Column>
											<Text text="Text"/>
										</Column>
									</columns>
									<items>
										<ColumnListItem type="Navigation" press="handelLineItemPress">
											<cells>
												<Text text="{ID}"/>
												<Text text="{LONG}"/>
												<Text text="{NAME}"/>
												<Text text="{BEEKEEPERID}"/>
											</cells>
										</ColumnListItem>
									</items>
								<customData>
								    <core:CustomData key="w5g.dt.editable" value="true" id="__data3"/>
								</customData>
								</Table>
							</IconTabFilter>
							
							<IconTabFilter text="Colonie">
								<Text text="Here you will see departing flights soon!"/>
							</IconTabFilter> 
						</items>
					</IconTabBar>

				</content>
			</Page>
		</pages>
	</App>
</mvc:View>