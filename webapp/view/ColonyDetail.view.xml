<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:f="sap.ui.layout.form" xmlns:l="sap.ui.layout" xmlns:html="http://www.w3.org/1999/xhtml" xmlns:smartchart="sap.ui.comp.smartchart" xmlns:sl="sap.ui.comp.navpopover" xmlns:viz="sap.viz.ui5.controls" xmlns:viz.feeds="sap.viz.ui5.controls.common.feeds" xmlns:viz.data="sap.viz.ui5.data" controllerName="BeeApp.controller.ColonyDetail">
	<App>
		<pages>
			<Page title="Title" showNavButton="true" navButtonPress="onNavBack">
				<content>
				<VBox class="sapUiSmallMargin">
				<f:SimpleForm id="flightDetails" title="Flight Details">
					<f:content>
						<RatingIndicator id="RI_default" maxValue="5" class="sapUiSmallMarginBottom" value="4" tooltip="Rating Tooltip"/>
						<Label text="{i18n>ColonyID}"/>
						<Input type="Text" width="100%" id="ID" value="{ID}" editable="false"/>
						<Label text="{i18n>ColonyName}Colony name"/>
						<Input type="Text" width="100%" id="NAME" value="{NAME}"/>
						<Label text="Colony long"/>
						<Input type="Text" width="100%" id="LONG" value="{LONG}"/>
						<Label text="Colony lat"/>
						<Input type="Text" width="100%" id="LAT" value="{LAT}"/>
					</f:content>
				</f:SimpleForm>
			</VBox>
					

	<VBox fitContainer="true">
		</VBox>
		
		
		
		
		
		
		</content>
				
				
				
				
				<headerContent>
				    <core:Icon src="sap-icon://warning2" size="2em" id="icon_alarm" backgroundColor="red"/>
				</headerContent>
				
									<footer>
  <Bar>
    <contentRight>
  
    <Button text="Accept" width="100px" id="__button1" type="Accept" press="onPressAccept"/>
  
    <Button text="Cancel" width="100px" id="__button0" type="Reject" press="onPressCancel"/>
  
    </contentRight>
  </Bar>
</footer>
				
				</Page></pages>
				
				
				
				</App></mvc:View>